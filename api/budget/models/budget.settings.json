{
  "kind": "collectionType",
  "collectionName": "budgets",
  "info": {
    "name": "budget"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 50
    },
    "money": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "budget_type": {
      "model": "budget-type",
      "via": "budgets"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "budgets"
    }
  }
}
