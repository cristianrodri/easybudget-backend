{
  "kind": "collectionType",
  "collectionName": "budget_types",
  "info": {
    "name": "budgetType"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true,
    "privateAttributes": ["created_at", "updated_at", "published_at", "user"]
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": ["income", "expense"],
      "required": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "budget_types"
    },
    "budgets": {
      "via": "budget_type",
      "collection": "budget"
    }
  }
}
